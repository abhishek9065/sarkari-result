{
    "root": "dist",
    "clean_urls": true,
    "routes": {
        "/**": "index.html"
    },
    "https_only": true,
    "headers": {
        "/**": {
            "Cache-Control": "public, max-age=31536000"
        },
        "/index.html": {
            "Cache-Control": "no-cache"
        }
    }
}