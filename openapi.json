  "components": {
    "schemas": {
      "ContentType": {
        "type": "string",
        "enum": [
          "job",
          "result",
          "admit-card",
          "syllabus",
          "answer-key",
          "admission"
        ]
      },
      "AnnouncementCard": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "title": {
            "type": "string"
          },
          "slug": {
            "type": "string"
          },
          "type": {
            "$ref": "#/components/schemas/ContentType"
          },
          "category": {
            "type": "string"
          },
          "organization": {
            "type": "string"
          },
          "location": {
            "type": "string",
            "nullable": true
          },
          "minQualification": {
            "type": "string",
            "nullable": true
          },
          "ageLimit": {
            "type": "string",
            "nullable": true
          },
          "salaryMin": {
            "type": "number",
            "nullable": true
          },
          "salaryMax": {
            "type": "number",
            "nullable": true
          },
          "difficulty": {
            "type": "string",
            "enum": [
              "easy",
              "medium",
              "hard"
            ],
            "nullable": true
          },
          "cutoffMarks": {
            "type": "string",
            "nullable": true
          },
          "deadline": {
            "type": "string",
            "format": "date-time",
            "nullable": true
          },
          "totalPosts": {
            "type": "integer",
            "nullable": true
          },
          "postedAt": {
            "type": "string",
            "format": "date-time"
          },
          "viewCount": {
            "type": "integer"
          },
          "isActive": {
            "type": "boolean"
          }
        },
        "required": [
          "id",
          "title",
          "slug",
          "type",
          "category",
          "organization",
          "postedAt",
          "viewCount",
          "isActive"
        ]
      },
      "Tag": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "slug": {
            "type": "string"
          }
        },
        "required": [
          "id",
          "name",
          "slug"
        ]
      },
      "ImportantDate": {
        "type": "object",
        "properties": {
          "eventName": {
            "type": "string"
          },
          "eventDate": {
            "type": "string"
          },
          "description": {
            "type": "string"
          }
        },
        "required": [
          "eventName",
          "eventDate"
        ]
      },
      "Announcement": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "title": {
            "type": "string"
          },
          "slug": {
            "type": "string"
          },
          "type": {
            "$ref": "#/components/schemas/ContentType"
          },
          "category": {
            "type": "string"
          },
          "organization": {
            "type": "string"
          },
          "content": {
            "type": "string"
          },
          "externalLink": {
            "type": "string"
          },
          "location": {
            "type": "string"
          },
          "deadline": {
            "type": "string",
            "format": "date-time"
          },
          "minQualification": {
            "type": "string"
          },
          "ageLimit": {
            "type": "string"
          },
          "applicationFee": {
            "type": "string"
          },
          "salaryMin": {
            "type": "number"
          },
          "salaryMax": {
            "type": "number"
          },
          "difficulty": {
            "type": "string",
            "enum": [
              "easy",
              "medium",
              "hard"
            ]
          },
          "cutoffMarks": {
            "type": "string"
          },
          "totalPosts": {
            "type": "integer"
          },
          "postedAt": {
            "type": "string",
            "format": "date-time"
          },
          "updatedAt": {
            "type": "string",
            "format": "date-time"
          },
          "isActive": {
            "type": "boolean"
          },
          "viewCount": {
            "type": "integer"
          },
          "tags": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Tag"
            }
          },
          "importantDates": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/ImportantDate"
            }
          },
          "jobDetails": {
            "type": "object",
            "additionalProperties": true
          }
        },
        "required": [
          "id",
          "title",
          "slug",
          "type",
          "category",
          "organization",
          "postedAt",
          "isActive",
          "viewCount"
        ]
      },
      "AnnouncementResponse": {
        "type": "object",
        "properties": {
          "data": {
            "$ref": "#/components/schemas/Announcement"
          }
        },
        "required": [
          "data"
        ]
      },
      "AnnouncementListResponse": {
        "type": "object",
        "properties": {
          "data": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Announcement"
            }
          },
          "count": {
            "type": "integer"
          }
        },
        "required": [
          "data"
        ]
      },
      "AnnouncementCardListResponse": {
        "type": "object",
        "properties": {
          "data": {
